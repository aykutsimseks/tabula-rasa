{
  "name": "tabula-rasa",
  "version": "1.0.0",
  "description": "Tabula Rasa",
  "main": "index.js",
  "scripts": {
    "migrate:up": "cd db; knex migrate:latest",
    "migrate:down": "cd db; knex migrate:rollback",
    "seed": "cd db; knex seed:run",
    "lint": "eslint src",
    "clean": "rm -rf public/build",
    "dev": "npm run clean && node server/server.js",
    "start": "npm run prod",
    "prod": "NODE_ENV=production PORT=3000 node ./server/server.js",
    "build:webpack": "NODE_ENV=production webpack --progress --config webpack/webpack.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "tableflip": "rm -rf public/build && rm -rf node_modules && rm -rf packages/*/node_modules && rm -rf scripts/*/node_modules",
    "install:all": "npm install && npm install packages/*/ && npm install scripts/*/",
    "install:server": "npm install webpack express http-proxy compression dotenv"
  },
  "author": "Aykut Simsek",
  "license": "MIT",
  "keywords": [],
  "dependencies": {
